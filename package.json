    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.8.6",
    "react-pure-lifecycle": "^3.0.0",
    "react-router": "5.0.1",
    "react-router-config": "5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/flux": "^3.1.9",
